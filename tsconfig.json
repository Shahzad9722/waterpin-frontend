{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "watch": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmitOnError": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "baseUrl": "./src"
  },
  "include": [
          "src/**/*.ts",
          "src/**/*.tsx",
          "src/**/*.js"
  ],
  "paths": {
    "@mui/styled-engine": [
      "./node_modules/@mui/styled-engine-sc"
    ]
  }
}
